<% songs = obj %>
<html>
<head>
  <%= head("Songs | G-ROCK") %>
</head>
<body>
  <%= header %>
  <div class="contents">
    <ul>
    <% songs.each do |code, song| %>
      <li>
        <a href="song/<%= song.code %>.html">
          <%= song.name %>
        </a>
      </li>
    <% end %>
    </ul>
  </div>
  <%= footer %>
</body>
</html>
