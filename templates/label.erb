<%= partial :main, {item: @label} %>

<div class="contents">
  <%= partial :body, {item: @label} %>
  <%= partial :sns, {item: @label} %>

  <%= partial :band, {item: @label, text: ["", @label.name]} %>

  <%= partial :text, {text: @label.founded, class_names: "active"} %>
  <%= partial :text, {text: @label.origin_full, class_names: "origin"} %>

  <% Label.site_keys.each do |site| %>
    <%= partial :site, {site: site, link_value: @label.send(site)} %>
  <% end %>
  <% @label.link_list.each do |site| %>
    <%= partial :site, {site: "link", link_value: site} %>
  <% end %>

  <%= partial :"admin/label" %>
</div>

<div class="sub-contents">
  <%= partial :feeds, {item: @label} %>
</div>
