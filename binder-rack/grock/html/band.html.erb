<% band = obj %>
<!DOCTYPE html>
<html lang="ja">
<head>
  <%= head band.name %>
</head>
<body>
  <%= header %>
  <div class="contents-wrapper">

    <%= main_video_block(band.videos.first) %>
    <%= main_image_block(band.images.first) if band.videos.empty? %>
    <h2><%= band.title %></h2>

    <div class="contents">

    </div>

    <div class="sub-contents">
      <%= feeds(band) %>
    </div>
  </div>
  <%= footer %>
</body>
</html>
