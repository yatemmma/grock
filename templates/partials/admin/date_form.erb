<label>
  <span><%= key %>: </span>
  <input form="form1" name="<%= key %>" type="text" onchange="onChangeText('<%= key %>', this)" value="<%= item.send(key.to_sym) %>" />
</label>
<label>
  <span> </span>
  <select onchange="onSelectDate(this, 'date')">
    <option value="">----</option>
    <% Setting.date_labels.to_hash.keys.each do |key| %>
      <option value="<%= key %>"><%= key %></option>
    <% end %>
  </select>
</label>
