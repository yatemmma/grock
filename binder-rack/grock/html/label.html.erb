<% label = obj %>
<!DOCTYPE html>
<html lang="ja">
<head>
  <%= head label.name %>
</head>
<body>
  <%= header %>
  <div class="contents-wrapper">

    <%= main_video_block(label.videos.first) %>
    <%= main_image_block(label.images.first) if label.videos.empty? %>
    <h2><%= label.name %></h2>

    <div class="contents">

    </div>

    <div class="sub-contents">
      <%= feeds(label) %>
    </div>
  </div>
  <%= footer %>
</body>
</html>
